</LinearLayout><TableRow android:id="@+id/TableRow01" android:layout_height="wrap_content" android:layout_marginRight="1dp" android:layout_width="fill_parent" android:layout_marginTop="370dp"><Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="center" android:textSize="20dp" android:paddingLeft="20dp" android:paddingRight="20dp" android:layout_marginRight="25dp" android:layout_marginLeft="7dp" android:text="@string/btnMod" android:id="@+id/cusInfo_btnMod"></Button><Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="center" android:paddingLeft="20dp" android:paddingRight="20dp" android:textSize="20dp" android:layout_marginRight="25dp" android:text="@string/btnDel" android:id="@+id/cusInfo_btnDel"></Button><Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="center" android:paddingLeft="20dp" android:paddingRight="20dp" android:textSize="20dp" android:layout_gravity="center" android:text="@string/btnOK" android:id="@+id/cusInfo_btnOK"></Button></TableRow></RelativeLayout>

<TextView android:text="전번 " android:id="@+id/cusInfo_tvNameValue" android:textSize="20dp" android:layout_height="wrap_content" android:layout_marginTop="4dp" android:layout_marginLeft="80dp" android:textColor="#000000" android:layout_width="170dp"></TextView>

